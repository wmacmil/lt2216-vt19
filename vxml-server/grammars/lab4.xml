<?xml version="1.0" encoding="UTF-8" ?>
<grammar version="1.0" tag-format="semantics/1.0" root="home">

  <rule id="home" scope="public"> 
    <count number="optional">
      <item>I want to go </item>
    </count>
    <one-of>

      <!-- <item> -->
      <!--   <ruleref uri="#t"/> -->
      <!--   <tag>out.to = rules.t;</tag> -->
      <!-- </item> -->
      <!-- <item> -->
      <!--   <ruleref uri="#f"/> -->
      <!--   <tag>out.to = rules.f;</tag> -->
      <!-- </item> -->
      <!-- <item> -->
      <!--   <ruleref uri="#d"/> -->
      <!--   <tag>out.to = rules.d;</tag> -->
      <!-- </item> -->
      <!-- <item> -->
      <!--   <ruleref uri="#r"/> -->
      <!--   <tag>out.to = rules.r;</tag> -->
      <!-- </item> -->


      <item>
        <ruleref uri="#t"/>
        <ruleref uri="#f"/>
        <tag>out.to = rules.t; out.from = rules.f; </tag>
      </item>
      <item>
        <ruleref uri="#f"/>
        <ruleref uri="#t"/>
        <tag>out.to = rules.t; out.from = rules.f; </tag>
      </item>

      <item>
        <ruleref uri="#t"/>
        <ruleref uri="#f"/>
        <ruleref uri="#d"/>
        <tag>out.to = rules.t; out.from = rules.f; out.date = rules.d;</tag>
      </item>

      <item>
        <ruleref uri="#f"/>
        <ruleref uri="#t"/>
        <ruleref uri="#d"/>
        <tag>out.to = rules.t; out.from = rules.f; out.date = rules.d;</tag>
      </item>

    </one-of>
  </rule>

  <rule id="t">
    <item>
      to 
      <ruleref uri="#city"/>
      <tag>out = rules.city;</tag>
    </item>
  </rule>

  <rule id="f">
    <item>
      from 
      <ruleref uri="#city"/>
      <tag>out = rules.city;</tag>
    </item>
  </rule>

  <rule id="d">
    <item>
      on 
      <ruleref uri="#month"/>
      <ruleref uri="#number"/>
      <tag>out = rules.month + " " + rules.number;</tag>
    </item>
  </rule>

  <rule id="r">
    <item>
      returning on 
      <ruleref uri="#month"/>
      <ruleref uri="#number"/>
      <tag>out = rules.month + " " + rules.number;</tag>
    </item>
  </rule>

  <rule id="city">
    <one-of>
       <item> New York <tag>out = "NY";</tag> </item>
       <item> Paris <tag>out = "Paris";</tag> </item>
       <item> London <tag>out = "London";</tag> </item>
    </one-of>
  </rule>

